@model IEnumerable<WebApplication008.Models.CalificacionesModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Inicio</h2>

<p>
    @Html.ActionLink("Crear Nueva", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id_usuario)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_materia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tipo_actividad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.calificacion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id_usuario)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_materia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tipo_actividad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.calificacion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id_calificacion }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id_calificacion }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id_calificacion })
        </td>
    </tr>
}

</table>
